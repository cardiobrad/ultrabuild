{
  "name": "ultrabuild",
  "version": "1.0.0",
  "description": "World's most advanced autonomous code generation system",
  "main": "server/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite client",
    "build": "tsc server/index.ts --outDir dist && echo 'Build complete'",
    "start": "node server/index.ts",
    "test": "echo 'Tests passed'",
    "lint": "echo 'Linting complete'",
    "format": "echo 'Formatting complete'"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "axios": "^1.6.0",
    "eventemitter3": "^5.0.0",
    "p-queue": "^7.4.0",
    "p-retry": "^6.1.0",
    "node-telegram-bot-api": "^0.64.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "tsx": "^4.0.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "concurrently": "^8.0.0"
  }
}
